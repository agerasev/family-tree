  "license": "MIT",
  "homepage": "https://github.com/agerasev/family-tree",
  "dependencies": {
    "@types/jquery": "^3.5.5",
    "browserify": "^17.0.0",
    "jquery": "^3.5.1",
    "sass": "^1.32.8",
    "sass3js": "^1.0.8",
    "toml": "^3.0.0",
    "ts-interface-builder": "^0.3.0",
    "ts-interface-checker": "^0.2.0",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "http-server": "^0.12.3"
  }
}
